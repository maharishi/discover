<div id='canvasctrl' class='input-group mb-3'>
    <label for='canvaslocation' />
    <input type='text' class='form-control' id='canvaslocation' placeholder='Enter Location' aria-label='Enter Location to search' />
</div>
<% if (buttons) { %>
<ul class='list-group'>
    <% buttons.forEach(function (button) { %>
        <li class='list-group-item'><%= button %></li>
    <% }) %>
</ul>
<% } %>
<script lang="javascript" type="text/javascript">
    $(".list-group-item").on("click", function(){
        // $(".list-group-item").each(function(n, elem){
        //     $( this ).removeClass("active");
        // });
        $( this ).toggleClass("active");
        evnts.map.onPlaceSearch.dispatch($( this ).text());
    })
    evnts.map.onInitMapSearchBox.dispatch();
</script>
